{
  "name": "cobudget-ui",
  "version": "0.3.0-pre",
  "description": "user interface for Cobudget!",
  "main": "./server.js",
  "browser": "./src/index.coffee",
  "scripts": {
    "install": "napa",
    "start": "gulp build && node .",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "test-spec": "testling -x firefox",
    "test-e2e": "protractor protractor.js",
    "test-style": "costandard",
    "test": "(export NODE_ENV=test; npm run test-style && npm run test-spec && npm run test-e2e)",
    "build": "gulp build",
    "develop": "gulp develop",
    "stage": "(export NODE_ENV=staging; gulp branch && git push origin gh-pages -f)",
    "deploy": "(export NODE_ENV=production; gulp branch && git push deploy gh-pages:master -f)"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/open-app/cobudget-ui.git"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "envify"
    ]
  },
  "testling": {
    "files": "src/node_modules/**/spec/**/*.coffee",
    "harness": "mocha",
    "browsers": [
      "ie/9..latest",
      "firefox/17..nightly",
      "chrome/23..canary",
      "opera/15..next",
      "safari/6..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/4.2"
    ]
  },
  "napa": {
    "angular-bootstrap": "angular-ui/bootstrap#gh-pages",
    "angular-xeditable": "vitalets/angular-xeditable#0.1.8",
    "awesome-bootstrap-checkbox": "flatlogic/awesome-bootstrap-checkbox#v0.2.3"
  },
  "costandard": {
    "ignore": [
      "build/**"
    ]
  },
  "dependencies": {
    "address-rfc2822": "0.0.2",
    "ampersand-collection": "^1.4.0",
    "ampersand-collection-lodash-mixin": "git://github.com/cobudget/ampersand-collection-lodash-mixin",
    "ampersand-state": "^4.4.4",
    "angular": "^1.3.2",
    "angular-bootstrap-datetimepicker": "^0.3.9",
    "angular-cookies": "^1.3.2",
    "angular-markdown-directive": "git://github.com/ushahidi/angular-markdown-directive",
    "angular-sanitize": "^1.3.8",
    "angular-ui-router": "^0.2.11",
    "babelify": "^5.0.3",
    "bignumber.js": "^2.0.0",
    "bootstrap-sass": "^3.3.1",
    "c3": "^0.3.0",
    "debug": "^2.1.0",
    "email-validator": "^1.0.1",
    "es5-shim": "^4.0.3",
    "font-awesome": "^4.2.0",
    "gulp-build-branch": "^0.1.4",
    "http-errors": "^1.3.0",
    "jquery": "^2.1.3",
    "json3": "^3.3.2",
    "lodash": "^2.4.1",
    "moment": "^2.8.4",
    "recase": "^1.0.4",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "brfs": "^1.2.0",
    "browserify": "^5.13.1",
    "browserify-ngannotate": "^0.7.1",
    "brstar": "^0.1.0",
    "bundle-collapser": "^1.1.0",
    "chai": "^1.10.0",
    "chai-as-promised": "^4.1.1",
    "coffee-script": "^1.8.0",
    "coffeeify": "^0.7.0",
    "connect-livereload": "^0.5.0",
    "costandard": "git://github.com/cobudget/costandard",
    "ecstatic": "^0.5.6",
    "envify": "^3.2.0",
    "express": "^4.10.2",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^1.0.1",
    "gulp-filter": "^1.0.2",
    "gulp-livereload": "^2.1.1",
    "gulp-nodemon": "^1.0.4",
    "gulp-plumber": "^0.6.6",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.1.0",
    "gulp-sourcemaps": "1.1.0",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^1.2.0",
    "mocha": "^2.0.1",
    "mold-source-map": "^0.3.0",
    "napa": "^1.0.2",
    "protractor": "^1.4.0",
    "sinon": "^1.10.3",
    "sinon-chai": "^2.5.0",
    "testling": "^1.7.1",
    "tiny-lr": "^0.1.4",
    "txtify2": "^2.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.1.1"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
