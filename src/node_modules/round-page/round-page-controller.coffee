### @ngInject ###
module.exports = ($scope, group, round) ->
    $scope.group = group
    $scope.round = round

    $scope.banner = {
      name: "round"
      parent: {
        name: group.name
        ref: "layout.group.rounds({ groupId: group.id })"
        logo: "/img/logo-enspiral.png"
      }
      child: {
        name: $scope.round.name
        ref: ".overview"
      }
    }

    $scope.tabs = [{
      name: "overview"
      label: "OVERVIEW"
    }, {
      name: "buckets"
      label: "BUCKETS"
    }, {
      name: "contributors"
      label: "CONTRIBUTORS"
    }]
