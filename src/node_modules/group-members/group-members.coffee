module.exports = {
  url: '/members'
  template: require('./group-members-template.html')
  controller: require('./group-members-controller.coffee')
  resolve:
    users: ($stateParams, UsersStore) ->
      UsersStore.all()
    memberships: ($stateParams, MembershipsStore) ->
      MembershipsStore.all($stateParams.groupId)
}
